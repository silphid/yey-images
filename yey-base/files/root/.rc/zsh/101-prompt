prompt_context() {
  trap '' SIGINT
  p10k segment -f blue -b black -i '‚≠êÔ∏è' -t "$YEY_CONTEXT"
}
prompt_time() {
  trap '' SIGINT
  p10k segment -f blue -b black -i 'üïì' -t "$(date +%X)"
}

POWERLEVEL9K_LEFT_PROMPT_ELEMENTS=(context dir vcs newline status)
POWERLEVEL9K_RIGHT_PROMPT_ELEMENTS=(time)
